syntax = "v1"

info (
	title:   "web manage"
	desc:    "user dashboard"
	author:  "xiaxuejin"
	version: "v1"
)

type (
	// 定义登录接口的 json 请求体
	CreateAppReq {
	}
	// 定义登录接口的 json 响应体
	CreateAppResp {
		AppID  int64  `json:"appid"`
		Secret string `json:"secret"`
	}
)
@server (
	maxBytes:   1048576
	middleware: WebAuthMiddleware
)

service web-api {
	@handler CreateApp
	post /app/create (CreateAppReq) returns (CreateAppResp)
}